SELECT * FROM clothingstore;
--data displayed ordered by price.

SELECT item, price FROM clothingstore ORDER BY price;

--what is the sales profit?

SELECT SUM(price) FROM clothingstore;

--what is the max price?

SELECT MAX(price) FROM clothingstore;

--what is the minimum price?

SELECT MIN(price) FROM clothingstore;
